{
  "name": "api-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "server": "babel-node ./src/bin/www",
    "server:prod": "node ./dist/bin/www",
    "dev": "SET NODE_ENV=development&& npm-run-all server",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "prod": "SET NODE_ENV=production npm-run-all clean build server:prod",
    "watch": "nodemon",
    "test": "jest"
  },
  "dependencies": {
    "bcrypt": "^3.0.0",
    "config": "^3.0.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "mongoose": "^5.0.1",
    "morgan": "~1.9.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.2",
    "babel-jest": "^24.7.1",
    "babel-preset-jest": "^24.6.0",
    "jest": "^24.7.1",
    "nodemon": "^1.18.10"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/"
  },
  "transform": {
    "^.+\\.jsx?$": "babel-jest"
  }
}
